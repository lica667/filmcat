<br /><%= link_to video.name, video_path(video) %>
<% if video.image? %>
	<br /><%= link_to image_tag(video.image.url, height: '271', width: '180', alt: '', class: "img-rounded"), video_path(video)  %>
<% else %>
	<br /><%= link_to image_tag(video.url_img, height: '271', width: '180', alt: '', class: "img-rounded"), video_path(video) %>
<% end %>
